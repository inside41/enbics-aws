<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.enbics.map.dao.MapDAO"> 
 
   <select id="selectMapList" resultType="com.enbics.vo.MapVO">
      <![CDATA[
			SELECT idtb_map,company,lat,lng FROM tb_map
      ]]>
   </select>
   
   <select id="selectDanjiList" resultType="com.enbics.vo.SanupdanjiVO">
      <![CDATA[
			SELECT dan_id,dan_name,dan_type,lat,lng FROM tb_sanupdanji
      ]]>
   </select>
   
</mapper>